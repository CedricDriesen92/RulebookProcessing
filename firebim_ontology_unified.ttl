fbo-DK:EvacuationTime a owl:ObjectProperty ;
    rdfs:label "Evacuation time"@en,
        "Evakueringstid"@en ;
    rdfs:seeAlso fbo-INT:EvacuationTime ;
    rdfs:subPropertyOf fbo:EvacuationTime ;
    fbo:hasArticleID "BR18-VK5-D01" ;
    fbo:hasCountryCode "DK" ;
    fbo:hasDefinition "Den tid, der går fra brandens start til alle personer i bygningen befinder sig på terræn i det fri eller et sikkert sted i bygningen. Evakueringstiden er summen af detekterings-, varslings-, reaktions- og beslutningstid samt bevægelsestid."@en,
        "The time from the start of the fire until all people in the building are on the terrain or in a safe refuge inside the building. Evacuation time is the sum of detection-, warning-, reaction-, decision and movement-time."@en ;
    fbo:hasValueType "Number" .

