    rdfs:subPropertyOf fbo:RoomOccupancy,
        fbo-DK:Roomfunction ;
    fbo:hasArticleID "BR18-VK5-D01" ;
    fbo:hasCountryCode "DK" ;
    fbo:hasDefinition "Rooms designed for occupancy, such as an office, meeting room, sales room, living room, bedroom, staff room or kitchen."@en,
        "Rum indrettet til personophold, f.eks. kontor, møderum, salgslokale, stue, soverum, personalerum eller køkken."@en ;
    fbo:hasUnit "Y/N" ;
    fbo:hasValueType "Boolean (yes/no)" .

