    fbo:hasArticleID "BR18-VK5-D01" ;
    fbo:hasCountryCode "DK" ;
    fbo:hasDefinition "Rooms for gathering several people, such as a theater hall, cinema hall, restaurant, banquet room, conference room, canteen, disco, nightclub, concert hall, exhibition hall, trade fair hall, sports hall, church hall and other rooms used for similar purposes."@en,
        "Rum til samling af flere personer, f.eks. teatersal, biografsal, restaurationslokale, selskabslokale, konferencerum, kantine, diskotek, natklub, koncertsal, udstillingslokale, messehal, idrætshal, kirkerum og andre lokaler, der anvendes til lignende formål."@en ;
    fbo:hasUnit "[-]" .

fbo-DK:RoomfunctionCanteen a owl:Class ;
