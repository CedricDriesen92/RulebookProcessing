    fbo:hasArticleID "Artikel 5.6.8" ;
    fbo:hasCountryCode "BE" ;
    fbo:hasDefinition "bouwlaag die de uitgang(en) bevat waardoor evacuatie naar buiten mogelijk is. Deze uitgangen leiden naar de openbare weg of naar een ruimte vanwaar die bereikbaar is."@en,
        "building layer containing the exit(s) allowing evacuation to the outside. These exits lead to the public road or to a space from which it is accessible."@en .

fbo-BE:EvacuationPath a owl:ObjectProperty ;
    rdfs:label "Path of evacuation"@en,
        "evacuatieweg"@en ;
    rdfs:subPropertyOf fbo:EvacuationPath ;
    fbo:hasArticleID "Artikel 5.6.1" ;
    fbo:hasCountryCode "BE" ;
    fbo:hasDefinition "maximum 10% hellende weg, binnen het gebouw, die toegang geeft tot trappenhuizen, vluchtterrassen of uitgangen"@en,
        "maximum 10% sloping road, within the building, that provides access to enclosed internal staircases, external staircases, vluchtterrassen and exits."@en ;
    fbo:hasUnit "m²" ;
    fbo:hasValueType "Number" .

fbo-BE:EvacuationPathOnEvacuationLevel a owl:Class ;
    rdfs:label "Evacuatieweg op het evacuatieniveau"@en,
        "Evacuation path on the evacuation level"@en ;
