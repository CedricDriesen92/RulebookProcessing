fbbo:correspondsToIFC a owl:ObjectProperty ;
    rdfs:label "corresponds to IFC"@en ;
    rdfs:comment "Links to the corresponding IFC entity"@en .

fbbo:hasBIMidsLink a owl:ObjectProperty ;
    rdfs:label "has BIMids link"@en ;
    rdfs:comment "Links to the BIMids.eu entry"@en .

fbbo:hasBSDDLink a owl:ObjectProperty ;
    rdfs:label "has bSDD link"@en ;
    rdfs:comment "Links to the buildingSMART Data Dictionary entry"@en .

fbbo:hasCountryCode a owl:DatatypeProperty ;
    rdfs:label "has country code"@en ;
    rdfs:comment "Specifies the country an entity is associated with"@en .

fbbo:hasDomain a owl:DatatypeProperty ;
    rdfs:label "has domain"@en ;
    rdfs:comment "Specifies the domain an entity belongs to"@en .

fbbo:hasIFCEntity a owl:DatatypeProperty ;
    rdfs:label "has IFC entity"@en ;
    rdfs:comment "Specifies the IFC entity name"@en .

fbbo:hasISODefinition a owl:DatatypeProperty ;
    rdfs:label "has ISO definition"@en ;
    rdfs:comment "Links an entity to its ISO definition"@en ;
    rdfs:subPropertyOf fbbo:hasDefinition .

fbbo:hasISOTermLink a owl:ObjectProperty ;
    rdfs:label "has ISO term link"@en ;
    rdfs:comment "Links to the ISO term definition"@en .

fbbo:hasRegion a owl:DatatypeProperty ;
    rdfs:label "has region"@en ;
    rdfs:comment "Specifies the region within a country"@en .

fbbo:hasRemark a owl:DatatypeProperty ;
    rdfs:label "has remark"@en ;
    rdfs:comment "Additional remarks about an entity"@en .

fbbo:hasType a owl:DatatypeProperty ;
    rdfs:label "has type"@en ;
    rdfs:comment "Specifies the type of a property"@en .

fbbo:hasUnit a owl:DatatypeProperty ;
    rdfs:label "has unit"@en ;
    rdfs:comment "Specifies the unit of measurement for a property"@en .

fbbo:hasValueType a owl:DatatypeProperty ;
    rdfs:label "has value type"@en ;
    rdfs:comment "Specifies the data type of a property's value"@en .

fbbo:isLinkedTo a owl:ObjectProperty ;
    rdfs:label "is linked to"@en ;
    rdfs:comment "Links a property to an object it describes"@en ;
    owl:inverseOf fbbo:hasProperty .

fbbo-BE:BuildingLayer a owl:Class ;
    rdfs:label "Building Layer"@en ;
    fbbo:hasCountryCode "BE" ;
    fbbo:hasDefinition "the space between a floor and the ceiling above it. The floors located below level Ei are basement floors and are not eligible for determining the number of floors of a building"@en,
        "de ruimte tussen een vloer en het daarboven liggend plafond. De bouwlagen gelegen onder het niveau Ei zijn kelderverdiepingen en komen niet in aanmerking voor het bepalen van het aantal bouwlagen van een gebouw"@nl-BE ;
    rdfs:subClassOf fbbo:BuildingLayer,
        fbbo-INT:BuildingStorey .

fbbo-BE:Evacuationpath a owl:Class ;
    rdfs:label "Evacuation path"@en ;
    fbbo:hasCountryCode "BE" ;
    fbbo:hasDefinition "maximum 10% sloping road, inside the building, that provides access to stairwells, emergency exits or exits"@en,
        "maximum 10% hellende weg, binnen het gebouw, die toegang geeft tot trappenhuizen, vluchtterrassen of uitgangen"@nl-BE ;
    rdfs:subClassOf fbbo:Evacuationpath,
        fbbo-INT:EvacuationRoute .

fbbo-BE:FireBIMOntology a owl:Ontology ;
    rdfs:label "FireBIM Building Ontology - Belgium" ;
