#################################################################
#    Object Properties
#################################################################

###  http://www.firebim.org/ontologies/fbo#hasDistance
fbo:hasDistance rdf:type owl:ObjectProperty ;
                rdfs:range fbo:Distance .


###  http://www.firebim.org/ontologies/fbo#hasMaterial
fbo:hasMaterial rdf:type owl:ObjectProperty ;
                rdfs:range fbo:Material ;
                rdfs:label "has material"@en ,
                           "heeft materiaal"@nl .


###  http://www.firebim.org/ontologies/fbo#hasTravelDistance
fbo:hasTravelDistance rdf:type owl:ObjectProperty ;
                      rdfs:subPropertyOf fbo:hasDistance ;
                      rdfs:domain <http://xmlns.com/foaf/0.1/Person> ;
                      rdfs:range fbo:Distance ;
                      fbo:hasISOEquivalent <https://www.iso.org/obp/ui/#iso:std:iso:13943:ed-4:v1:en:term:3.456> ;
                      rdfs:label "has travel distance"@en ,
                                 "heeft reisafstand"@nl .


###  http://www.firebim.org/ontologies/fbo#hasUsageFunction
fbo:hasUsageFunction rdf:type owl:ObjectProperty ;
                     rdfs:range fbo:UsageFunction .


###  http://www.firebim.org/ontologies/fbo-dk#hasActualTravelDistance
fbo-dk:hasActualTravelDistance rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf fbo:hasTravelDistance ;
                               rdfs:range fbo:Distance ;
                               fbo:hasExplicitDefinition "Den faktiske afstand, som en person skal gå i ganglinje for at nå et givent punkt. Ganglinjen udlægges som den direkte vej, under hensyn til faste forhindringer eller fast inventar, og på trædefast areal. Anvendes også om den fysiske længde af en gang."@da ,
                                                         "The actual distance that a person must walk in a walking line to reach a given point. The walking line is laid out as the direct route, taking into account fixed obstacles or fixtures, and on a walkable surface. Also used to refer to the physical length of a corridor."@en ;
                               rdfs:label "har ganglængde"@da ,
                                          "has actual travel distance"@en ,
                                          "heeft werkelijke loopafstand"@nl .


###  http://www.firebim.org/ontologies/fbo-dk#hasBlindEndDistance
fbo-dk:hasBlindEndDistance rdf:type owl:ObjectProperty ;
                           rdfs:subPropertyOf fbo:hasTravelDistance ;
                           fbo:hasExplicitDefinition "Gangafstand målt via gangarealer, der har adgang til en udgang i én retning. Afstanden måles parallelt med væggen og fra midt dør til midt dør."@da ,
                                                     "Walking distance measured via walkways that have access to an exit in one direction. Distance is measured parallel to the wall and from centerline of one door tocenterline of next door."@en ;
                           rdfs:label "har længde på blind ende"@da ,
                                      "has blind end distance"@en .


###  http://www.firebim.org/ontologies/fbo-dk#hasDirectTravelDistance
fbo-dk:hasDirectTravelDistance rdf:type owl:ObjectProperty ;
                               rdfs:subPropertyOf fbo:hasTravelDistance ;
                               rdfs:range fbo:Distance ;
                               fbo:hasExplicitDefinition "Den teoretisk længste afstand, som en person skal tilbagelægge for at nå frem til nærmeste udgang eller udgangsdør ved at bevæge sig langs ganglinjer. Ganglinjer udlægges svarende til den mest ugunstige placering af personen. I lokaler skal afstanden via ganglinjer, der kun fører til udgangsdør i én retning, multipliceres med 1,5."@da ,
                                                         "The theoretical longest distance a person must travel to reach the nearest exit or exit door by moving along a path of travel. The paths are laid out corresponding to the most unfavorable location of the person. In rooms, the distance along paths leading to the exit door in one direction only should be multiplied by 1.5."@en ;
                               rdfs:label "har gangafstand"@da ,
                                          "has direct travel distance"@en ,
                                          "heeft theoretische loopafstand"@nl .


###  http://www.firebim.org/ontologies/fbo-dk#hasRoomFunction
fbo-dk:hasRoomFunction rdf:type owl:ObjectProperty ;
                       rdfs:subPropertyOf fbo:hasUsageFunction ;
                       rdfs:range fbo-dk:RoomFunction .


###  http://www.firebim.org/ontologies/fbo-nl#hasEscapeRouteFreePassage
fbo-nl:hasEscapeRouteFreePassage rdf:type owl:ObjectProperty ;
                                 schema:domainIncludes fbo:Door ,
                                                       fbo:EscapeRoute ;
                                 fbo:hasExplicitDefinition "De hoogte en breedte van ruimten waardoor een vluchtroute voert zowel als de hoogte en breedte van deuropeningen (doorgangen)."@en ,
                                                           "The height and width of spaces through which an escape route passes, as well as the height and width of doorways (passages)."@en ;
                                 rdfs:label "has escape route free passage"@en ,
                                            "heeft vrije doorgang van vluchtroute"@nl .


###  http://www.firebim.org/ontologies/fbo-nl#isMirrorSymmetric
fbo-nl:isMirrorSymmetric rdf:type owl:ObjectProperty ;
                         rdfs:domain fbo:Building ;
                         rdfs:range fbo:Building ;
                         fbo:hasExpertDefinition "Building located on another plot of land identical to a building that is mirrored in relation to the plot boundary."@en ,
                                                 "Op een ander perceel gelegen gebouw van een identiek maar gespiegeld ten opzichte van de perceelsgrens gelegen gebouw."@nl ;
                         rdfs:label "is mirror-symmetric"@en ,
                                    "is spiegelsymmetrisch"@nl .


###  http://www.firebim.org/ontologies/fbo-pt#hasDominantOccupancyType
fbo-pt:hasDominantOccupancyType rdf:type owl:ObjectProperty ;
                                rdfs:subPropertyOf fbo:hasUsageFunction ;
                                rdfs:range fbo-pt:OccupancyType ;
                                fbo:hasExplicitDefinition "This is the use which, among the various uses of its spaces, defines the purpose that allows it to be classified as a specific occupancy type(UT I to UT XII)."@en ,
                                                          "É aquele que, de entre os diversos usos dos seus espaços, define a finalidade que permite atribuir a classificação de determinada utilização -tipo(UT I a UT XII)."@pt ;
                                rdfs:label "has dominant occupancy type"@en ,
                                           "tem uso dominante de uma utilização-tipo"@pt .


###  http://www.firebim.org/ontologies/fbo-pt#hasExclusiveOccupancyType
fbo-pt:hasExclusiveOccupancyType rdf:type owl:ObjectProperty ;
                                 rdfs:subPropertyOf fbo:hasUsageFunction ;
                                 rdf:type owl:FunctionalProperty ;
                                 rdfs:range fbo-pt:OccupancyType ;
                                 fbo:hasExplicitDefinition "Atendendo ao seu uso, os edifícios e recintos podem ser de utilização exclusiva, quando integrem uma única utilização-tipo."@pt ,
                                                           "Depending on their use, buildings and premises may be for exclusive use when they are integrated into a single type of occupancy."@en ;
                                 rdfs:label "has exclusive occupancy type"@en ,
                                            "tem utilização tipo exclusiva"@pt .


###  http://www.firebim.org/ontologies/fbo-pt#isDistinctExit
fbo-pt:isDistinctExit rdf:type owl:ObjectProperty ,
                               owl:SymmetricProperty ;
                      fbo:hasExplicitDefinition "Exits for which, from that point, routes can be established for both, taking into account the main fixed furniture and equipment or the highlighted paths, when required by these Regulations, diverging from an angle greater than 45°, measured in plan."@en ,
                                                "Saídas para as quais, a partir desse ponto, se possam estabelecer linhas de percurso para ambas, tendo em conta o mobiliário principal fixo e o equipamento ou os caminhos evidenciados, quando este Regulamento os exigir, divergindo de um ângulo superior a 45°, medido em planta."@pt ;
                      rdfs:label "is distinct exit"@en ,
                                 "é saída distintas em relação a um ponto"@pt .


