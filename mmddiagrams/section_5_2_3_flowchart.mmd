graph TD
    start([Start]) --> check_parking{{"Check if <font color='pink'>Parking</font> is adjacent to another building"}}
    check_parking -->|Yes| no_wall{{"Is there a separating wall?"}}
    check_parking -->|No| end([End])
    no_wall -->|No| single_parking[["Treat as single <font color='pink'>Parking</font>"]]
    no_wall -->|Yes| end
    single_parking --> check_structural_elements{{"Check all structural elements of the entire <font color='pink'>Parking</font>"}}
    check_structural_elements --> check_fire_resistance{{"Does each element have <font color='blue'>Fire resistance</font> R 120?"}}
    check_fire_resistance -->|Yes| pass([Pass])
    check_fire_resistance -->|No| fail([Fail])
    pass --> end
    fail --> end
    style start fill:#f9f,stroke:#333,stroke-width:2px
    style end fill:#f9f,stroke:#333,stroke-width:2px
    style pass fill:#9f9,stroke:#333,stroke-width:2px
    style fail fill:#f99,stroke:#333,stroke-width:2px