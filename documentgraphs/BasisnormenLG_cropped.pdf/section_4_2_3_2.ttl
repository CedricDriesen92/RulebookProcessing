firebim:Section_4_2_3_2 a firebim:Section ;
    firebim:hasID "4.2.3.2" ;
    firebim:hasOriginalText "Nuttige breedte van <a href=\"http://example.com/firebimbuilding#Staircase\">traparmen</a>, <a href=\"http://example.com/firebimbuilding#Landing\">overlopen</a> en sassen."@nl ;
