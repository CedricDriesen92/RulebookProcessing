firebim:Section_4_2_2_5 a firebim:Section ;
    firebim:hasID "4.2.2.5" ;
    firebim:hasOriginalText """De <a href="http://example.com/firebimbuilding#Staircase">trappenhuizen</a> die de <a href="http://example.com/firebimbuilding#UndergroundFloor">ondergrondse bouwlagen</a> bedienen, mogen niet rechtstreeks het verlengde zijn van degene die de <a href="http://example.com/firebimbuilding#Floor">bouwlagen</a> boven een <a href="http://example.com/firebimbuilding#EvacuationLevel">evacuatieniveau</a> bedienen.
Dit sluit niet uit dat het ene boven het andere mag liggen, mits volgende voorwaarden:"""@nl ;
    firebim:hasArticle firebim:Article_4_2_2_5_1 .

firebim:Section_4_2_2 firebim:hasSection firebim:Section_4_2_2_5 .

firebim:Article_4_2_2_5_1 a firebim:Article ;
    firebim:hasID "A4.2.2.5.1" ;
    firebim:hasMember firebim:Member_4_2_2_5_1_1, firebim:Member_4_2_2_5_1_2 .

firebim:Member_4_2_2_5_1_1 a firebim:Member ;
    firebim:hasOriginalText """1. de <a href="http://example.com/firebimbuilding#SeparationWall">wanden</a> die ze scheiden hebben <a href="http://example.com/firebimbuilding#WallFireResistance">EI 60</a>."""@nl .

firebim:Member_4_2_2_5_1_2 a firebim:Member ;
    firebim:hasOriginalText """2. de <a href="http://example.com/firebimbuilding#Access">toegang</a> tot ieder <a href="http://example.com/firebimbuilding#Staircase">trappenhuis</a> geschiedt via een <a href="http://example.com/firebimbuilding#IsSelfClosing">zelfsluitende</a> of <a href="http://example.com/firebimbuilding#IsFireActivatedClosing">bij brand zelfsluitende</a> <a href="http://example.com/firebimbuilding#Door">deur</a> <a href="http://example.com/firebimbuilding#DoorFireResistance">EI1 30</a>."""@nl .