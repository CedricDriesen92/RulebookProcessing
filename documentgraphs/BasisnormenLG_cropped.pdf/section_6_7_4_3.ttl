firebim:Section_6_7_4_3 a firebim:Section ;
    firebim:hasID "6.7.4.3" ;
    firebim:hasOriginalText "Plaatsing van de klep"@nl ;
    firebim:hasArticle firebim:Article_6_7_4_3_1 .

firebim:Article_6_7_4_3_1 a firebim:Article ;
