fro:Section_5_2_1 a fro:Section ;
    fro:hasID "5.2.1" ;
    fro:hasOriginalText "Structurele elementen"@nl ;
    fro:hasArticle fro:Article_5_2_1_1 .

fro:Article_5_2_1_1 a fro:Article ;
    fro:hasID "A5.2.1.1" ;
    fro:hasMember fro:Member_5_2_1_1_1,
        fro:Member_5_2_1_1_2,
        fro:Member_5_2_1_1_3 .

fro:Member_5_2_1_1_1 a fro:Member ;
    fro:hasOriginalText """In afwijking van het punt 3.2 hebben de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> R 120 en de <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">vloeren</a> van de parkeerbouwlagen en van de hellingen hebben R 120."""@nl .

fro:Member_5_2_1_1_2 a fro:Member ;
    fro:hasOriginalText """Wanneer het <a href=\"https://ontology.firebim.be/ontology/fbo#Roof\">dak</a> geen andere functie heeft dan alleen het beschermen van de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> tegen de weersomstandigheden:"""@nl ;
    fro:hasMember fro:Member_5_2_1_1_2_1,
        fro:Member_5_2_1_1_2_2 .

fro:Member_5_2_1_1_2_1 a fro:Member ;
    fro:hasOriginalText "- hebben de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#Roof\">dak</a> R 30;"@nl .

fro:Member_5_2_1_1_2_2 a fro:Member ;
    fro:hasOriginalText """- of worden de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#Roof\">dak</a> gescheiden van de rest van de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> door een <a href=\"https://ontology.firebim.be/ontology/fbo#BuildingElement\">bouwelement</a> EI 30."""@nl .

fro:Member_5_2_1_1_3 a fro:Member ;
    fro:hasOriginalText """Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#OpenParking\">open parkeerbouwlagen</a> worden de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> aan geen enkele vereiste onderworpen inzake <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a>, op voorwaarde:"""@nl ;
    fro:hasMember fro:Member_5_2_1_1_3_1,
        fro:Member_5_2_1_1_3_2 .

fro:Member_5_2_1_1_3_1 a fro:Member ;
    fro:hasOriginalText """- dat de <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">vloeren</a> van deze <a href=\"https://ontology.firebim.be/ontology/fbo#OpenParking\">open parkeerbouwlagen</a> en de hellingen tussen deze <a href=\"https://ontology.firebim.be/ontology/fbo#OpenParking\">open parkeerbouwlagen</a> minstens REI 60 hebben;"""@nl .

fro:Member_5_2_1_1_3_2 a fro:Member ;
    fro:hasOriginalText """- en dat deze <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> geen ander <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> dragen."""@nl .