firebim:Section_6_8_2_2 a firebim:Section ;
    firebim:hasID "6.8.2.2" ;
    firebim:hasOriginalText "DE TOESTELLEN DIE MENSELIJKE INTERVENTIE VERGEN, WORDEN AANGEBRACHT OP ZICHTBARE OF HELDER AANGEDUIDE PLAATSEN DIE IN ALLE OMSTANDIGHEDEN VRIJ BEREIKBAAR ZIJN. ZIJ BEVINDEN ZICH ONDER MEER NABIJ UITGANGEN, OP OVERLOPEN, IN GANGEN EN WORDEN DERWIJZE AANGEBRACHT DAT ZIJ DE CIRCULATIE NIET HINDEREN EN NIET BESCHADIGD OF AANGESTOTEN KUNNEN WORDEN." .

firebim:Section_6_8_2_3 a firebim:Section ;
    firebim:hasID "6.8.2.3" ;
