        fbd:Room,
        fbd:Wall ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationSafety\">evacuatie</a> van <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> of een geheel van <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> met <a href=\"https://ontology.firebim.be/ontology/fbo#NightOccupancy\">nachtbezetting</a> gebeurt via <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> waarvan de <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">verticale wanden</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI 30</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI1 30</a> hebben."@nl .

fbd:Member_4_4_1_2_1_11 a fbd:Member ;
