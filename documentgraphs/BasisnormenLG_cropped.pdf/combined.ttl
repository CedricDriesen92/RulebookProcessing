fbd:Member_6_8_5_3_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:FireHoseReel,
        fbd:TotalFloorArea ;
    fbd:hasMember fbd:Member_6_8_5_3_1_1_2_1,
        fbd:Member_6_8_5_3_1_1_2_2 ;
    fbd:hasOriginalText "Indien de <a href=\"https://ontology.firebim.be/ontology/fbo#TotalFloorArea\">oppervlakte van een gebouw</a> kleiner is dan 500 m² is geen <a href=\"https://ontology.firebim.be/ontology/fbo#FireHoseReel\">muurhaspel</a> verplicht (behoudens bij speciale risico’s). In alle andere gevallen wordt het aantal <a href=\"https://ontology.firebim.be/ontology/fbo#FireHoseReel\">muurhaspels</a> als volgt bepaald:"@nl .

fbd:Member_6_8_5_3_1_1_2_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:WaterFlowReach ;
    fbd:hasOriginalText "1. de <a href=\"https://ontology.firebim.be/ontology/fbo#WaterFlowReach\">waterstraal</a> bereikt elk punt van een <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a>;"@nl .

fbd:Member_6_8_5_3_1_1_2_2 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:FireHoseReel ;
    fbd:hasOriginalText "2. <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> groter dan 500 m² beschikken over ten minste 1 <a href=\"https://ontology.firebim.be/ontology/fbo#FireHoseReel\">muurhaspel</a>."@nl .

fbd:Member_6_8_5_3_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:FireHydrant,
        fbd:FirefighterAction ;
    fbd:hasOriginalText "Het perskoppelstuk van de eventuele <a href=\"https://ontology.firebim.be/ontology/fbo#FireHydrant\">muurhydranten</a> is aangepast aan de koppelingen gebruikt door de <a href=\"https://ontology.firebim.be/ontology/fbo#FirefighterAction\">brandweer</a>."@nl .

fbd:Member_6_8_5_4_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Aboveground,
        fbd:FireHydrant,
        fbd:Pipe,
        fbd:PipeDiameter,
        fbd:Underground,
        fbd:WaterSupplySystem ;
    fbd:hasOriginalText "Deze <a href=\"https://ontology.firebim.be/ontology/fbo#Aboveground\">bovengrondse</a> en <a href=\"https://ontology.firebim.be/ontology/fbo#Underground\">ondergrondse</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireHydrant\">hydranten</a> worden gevoed door het openbaar <a href=\"https://ontology.firebim.be/ontology/fbo#WaterSupplySystem\">waterleidingnet</a> via een <a href=\"https://ontology.firebim.be/ontology/fbo#Pipe\">leiding</a> met minimale <a href=\"https://ontology.firebim.be/ontology/fbo#PipeDiameter\">binnendiameter</a> van 80 mm."@nl .

fbd:Member_6_8_5_4_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:AutomaticFireExtinguishingSystem,
        fbd:Building,
        fbd:FireRisk,
        fbd:SprinklerSystem ;
    fbd:hasOriginalText "Kan het openbaar net aan deze voorwaarden niet voldoen, dan wendt men andere bevoorradingsbronnen aan met minimale capaciteit van 50 m³, tenzij het gehele <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> is uitgerust met een <a href=\"https://ontology.firebim.be/ontology/fbo#AutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> van het type <a href=\"https://ontology.firebim.be/ontology/fbo#SprinklerSystem\">sprinkler</a> die is aangepast aan de aanwezige <a href=\"https://ontology.firebim.be/ontology/fbo#FireRisk\">risico's</a>."@nl .

