fbd:Member_6_7_1_3_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Air,
        fbd:BoilerRoom,
        fbd:Kitchen,
        fbd:Parking,
        fbd:TransformerRoom,
        fbd:WasteStorageRoom ;
    fbd:hasOriginalText "De <a href=\"http://example.com/firebimbuilding#Air\">lucht</a> afgezogen uit lokalen met een bijzonder brandgevaar, opslagplaats voor ontvlambare producten, <a href=\"http://example.com/firebimbuilding#BoilerRoom\">stookplaats</a>, <a href=\"http://example.com/firebimbuilding#Kitchen\">keuken</a>, <a href=\"http://example.com/firebimbuilding#Parking\">garage</a>, <a href=\"http://example.com/firebimbuilding#Parking\">parking</a>, <a href=\"http://example.com/firebimbuilding#TransformerRoom\">transformatorlokaal</a>, <a href=\"http://example.com/firebimbuilding#WasteStorageRoom\">lokaal voor de opslag van vuilnis</a>, mag niet opnieuw worden rondgestuurd en moet naar buiten worden afgevoerd."@nl .

fbd:Member_6_7_1_3_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Air ;
    fbd:hasMember fbd:Member_6_7_1_3_1_2_1,
        fbd:Member_6_7_1_3_1_2_2 ;
    fbd:hasOriginalText "De <a href=\"http://example.com/firebimbuilding#Air\">lucht</a> afgezogen uit andere lokalen mag:"@nl .

fbd:Member_6_7_1_3_1_2_1 a fbd:Member ;
    fbd:hasKeyword fbd:SmokeValve ;
    fbd:hasOriginalText "- ofwel opnieuw rondgestuurd worden naar dezelfde lokalen, op voorwaarde dat in het recyclagekanaal een <a href=\"http://example.com/firebimbuilding#SmokeValve\">rookklep</a> conform punt 6.7.5 wordt geplaatst;"@nl .

fbd:Member_6_7_1_3_1_2_2 a fbd:Member ;
    fbd:hasKeyword fbd:AirDuct,
        fbd:SmokeValve ;
