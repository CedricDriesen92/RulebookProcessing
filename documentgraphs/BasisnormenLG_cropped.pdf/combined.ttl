fbd:Member_3_4_2_1_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:MaterialClass ;
    fbd:hasOriginalText "-    zij bestaan uit een <a href=\"http://example.com/firebimbuilding#MaterialClass\">materiaal van klasse</a> A1 en/of A2-s1,d0;"@nl .

fbd:Member_3_4_2_1_2_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:CeilingVoid ;
    fbd:hasOriginalText "-    zij beslaan de gehele <a href=\"http://example.com/firebimbuilding#CeilingVoid\">ruimte tussen de leidingen</a>;"@nl .

