        fbd:IsSelfClosing,
        fbd:Room,
        fbd:Wall ;
    fbd:hasOriginalText "- een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatieweg</a> op een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationLevel\">evacuatieniveau</a> waarvan de <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">verticale binnenwanden</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI 60</a> hebben en de <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> die op deze wegen uitgeven <a href=\"https://ontology.firebim.be/ontology/fbo#IsSelfClosing\">zelfsluitend</a> of <a href=\"https://ontology.firebim.be/ontology/fbo#IsFireActivatedClosing\">bij brand zelfsluitend</a> zijn en <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI1 30</a> hebben."@nl .

fbd:Member_4_4_1_2_1_5 a fbd:Member ;
