fbd:Member_4_4_1_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationRoute ;
    fbd:hasOriginalText "De lengte van doodlopende <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> mag niet meer dan 15 m bedragen."@nl .

fbd:Member_4_4_1_1_1_4 a fbd:Member ;
    fbd:hasKeyword fbd:Door,
        fbd:EscapeTerrace,
        fbd:EvacuationRoute,
        fbd:UsefulWidth ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#UsefulWidth\">nuttige breedte</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a>, <a href=\"https://ontology.firebim.be/ontology/fbo#EscapeTerrace\">vluchtterrassen</a> en van hun toegangs-, uitgangs- of doorgangs<a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a> is groter dan of gelijk aan de vereiste <a href=\"https://ontology.firebim.be/ontology/fbo#UsefulWidth\">nuttige breedte</a> (zie bijlage 1 \"Terminologie\"). Zij bedraagt ten minste 0,80 m voor de <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a>, en ten minste 0,60 m voor de <a href=\"https://ontology.firebim.be/ontology/fbo#EscapeTerrace\">vluchtterrassen</a>."@nl .

fbd:Member_4_4_1_1_1_5 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:EscapeTerrace,
        fbd:EvacuationRoute,
        fbd:Staircase ;
    fbd:hasOriginalText "In een <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> gaat de verbinding tussen en naar de <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappen</a> via <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> of over <a href=\"https://ontology.firebim.be/ontology/fbo#EscapeTerrace\">vluchtterrassen</a>."@nl .

fbd:Member_5_1_2_1_1 fbd:hasMember fbd:Member_5_1_2_1_1_6 .

fbd:Member_6_7_3_2_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:AirDuct,
        fbd:AirHandlingUnit,
        fbd:Shaft,
        fbd:TechnicalRoom,
        fbd:Wall ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#AirDuct\">luchtkanalen</a> die zich in uitsluitend voor hen voorbehouden <a href=\"https://ontology.firebim.be/ontology/fbo#Shaft\">kokers</a> bevinden en aan hun boveneinde uitmonden in een <a href=\"https://ontology.firebim.be/ontology/fbo#TechnicalRoom\">technisch lokaal</a> dat enkel de <a href=\"https://ontology.firebim.be/ontology/fbo#AirHandlingUnit\">luchtbehandelingsgroepen</a> bevat die zij verbinden, mogen zonder bijkomende voorzieningen door de <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#TechnicalRoom\">technisch lokaal</a> gaan. In dat geval dient de verluchting van de <a href=\"https://ontology.firebim.be/ontology/fbo#Shaft\">kokers</a> zoals vereist in 5.1.5.1 verwezenlijkt te worden via het <a href=\"https://ontology.firebim.be/ontology/fbo#TechnicalRoom\">technisch lokaal</a>."@nl .

