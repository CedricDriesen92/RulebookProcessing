        fbd:OpenParking,
        fbd:Parking,
        fbd:StructuralElement ;
    fbd:hasOriginalText "In dat geval hebben de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van de gehele <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">R 120</a>, met inbegrip van de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#OpenParking\">open parkeerbouwlagen</a>."@nl .

fbd:Member_5_2_4_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Parking ;
    fbd:hasOriginalText "Het ontwerp, de bouw en de inrichting van de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> voldoen aan de bepalingen van het punt 3 van de bijlage 7."@nl ;
    fbd:hasReference fbd:Reference_Bijlage_7 .

fbd:Member_5_2_5_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Parking ;
    fbd:hasOriginalText "De punten 5.2.1 tot 5.2.4 zijn niet van toepassing op de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> waarvoor de aanvraag voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">bouw</a> werd ingediend vóór 1 juli 2022 als het voldoet aan de volgende voorwaarden."@nl .

fbd:Member_5_2_5_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:FireResistance,
        fbd:OpenParking,
        fbd:StructuralElement,
        fbd:Wall ;
    fbd:hasOriginalText "In afwijking van het punt 3.2, worden er geen eisen voor <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> gesteld aan de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> van <a href=\"https://ontology.firebim.be/ontology/fbo#OpenParking\">open parkings</a> waarvan de horizontale <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> REI 60 bezitten."@nl .

fbd:Member_5_2_5_1_3 a fbd:Member ;
