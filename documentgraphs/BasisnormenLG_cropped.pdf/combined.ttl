        fbd:Section_4_5 ;
    fbd:hasKeyword fbd:Compartment,
        fbd:EvacuationLevel,
        fbd:EvacuationRoute,
        fbd:ExternalStaircase,
        fbd:Floor,
        fbd:HasExitToOutside,
