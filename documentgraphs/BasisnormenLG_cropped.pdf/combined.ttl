    fbd:hasOriginalText "     -    45 m van de toegang tot de dichtstbijzijnde <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgang</a>;"@nl .

fbd:Member_4_4_1_1_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Exits ;
    fbd:hasOriginalText "     -    80 m van de toegang tot een tweede <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgang</a>."@nl .

