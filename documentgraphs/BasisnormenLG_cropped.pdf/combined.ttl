fbd:Member_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:AccessRoad,
        fbd:FireDepartmentApproval ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#AccessRoad\">toegangswegen</a> bedoeld in punt 1.1 worden bepaald in <a href=\"https://ontology.firebim.be/ontology/fbo#FireDepartmentApproval\">akkoord met de brandweer</a>, volgens de volgende leidraad."@nl .

fbd:Member_1_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Facade,
        fbd:Floor ;
    fbd:hasOriginalText "Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouwen</a> met één <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> moeten de voertuigen van de brandweer ten minste tot op 60 m van een <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> kunnen naderen."@nl .

fbd:Member_1_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:AccessToEvacuationLevel,
        fbd:Building,
        fbd:Facade,
        fbd:Floor ;
    fbd:hasOriginalText "Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouwen</a> met meer dan één <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> moeten de voertuigen van de brandweer ten minste in één punt een <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a> kunnen bereiken die op herkenbare plaatsen <a href=\"https://ontology.firebim.be/ontology/fbo#AccessToEvacuationLevel\">toegang geeft tot</a>"@nl .

fbd:Member_1_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationSafety,
        fbd:FirefighterAction,
        fbd:UserSafety ;
    fbd:hasOriginalText "Bijgebouwen, uitspringende daken, luifels, uitkragende delen of andere dergelijke toevoegingen zijn enkel toegelaten indien daardoor noch de <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationSafety\">evacuatie</a>, noch de <a href=\"https://ontology.firebim.be/ontology/fbo#UserSafety\">veiligheid van de gebruikers</a>, noch de <a href=\"https://ontology.firebim.be/ontology/fbo#FirefighterAction\">actie van de brandweer</a> in het gedrang komen."@nl .

fbd:Member_1_3_1_1 a fbd:Member ;
    fbd:hasMember fbd:Member_1_3_1_1_1,
        fbd:Member_1_3_1_1_2 .

fbd:Member_1_3_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:AngleBetweenFacades,
        fbd:BuildingHeight,
        fbd:DistanceBetweenBuildings,
        fbd:Facade,
        fbd:FireResistanceRating ;
    fbd:hasMember fbd:Member_1_3_1_1_1_1 ;
    fbd:hasOriginalText """a) ofwel, als <a href="https://ontology.firebim.be/ontology/fbo#Facade">gevels</a> tegenover elkaar staan of een inspringende tweevlakshoek vormen, dan bedraagt de <a href="https://ontology.firebim.be/ontology/fbo#DistanceBetweenBuildings">afstand</a> (in m) tussen de <a href="https://ontology.firebim.be/ontology/fbo#Facade">geveldelen</a> die niet minstens <a href="https://ontology.firebim.be/ontology/fbo#FireResistanceRating">EI 60</a> of <a href="https://ontology.firebim.be/ontology/fbo#FireResistanceRating">REI 60</a> hebben, ten minste:

|Hoek (α)|Afstand (m)|
|---|---|
|0° ≤ α ≤ 90°|h + 10cos α|
|90° < α ≤ 180°|0|

waarbij α de <a href="https://ontology.firebim.be/ontology/fbo#AngleBetweenFacades">ingesloten hoek</a> is en h de <a href="https://ontology.firebim.be/ontology/fbo#BuildingHeight">hoogte van het gebouw</a> in m (zie plaat 2.1)."""@nl .

fbd:Member_1_3_1_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Building ;
    fbd:hasOriginalText "Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouwen</a> waarvoor de aanvraag voor de bouw werd ingediend voor 1 juli 2022, is"@nl .

fbd:Member_1_3_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:RadiationLimit ;
    fbd:hasOriginalText "b) ofwel mag de <a href=\"https://ontology.firebim.be/ontology/fbo#RadiationLimit\">straling van een brand</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> op een tegenoverstaand <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a>, en omgekeerd, niet meer dan 15 kW/m² bedragen."@nl .

fbd:Member_1_3_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Door,
        fbd:Fire,
        fbd:FireResistanceRating,
        fbd:SeparationWall,
        fbd:Wall ;
    fbd:hasOriginalText """De <a href="https://ontology.firebim.be/ontology/fbo#SeparationWall">wanden die aangrenzende gebouwen scheiden</a> hebben <a href="https://ontology.firebim.be/ontology/fbo#FireResistanceRating">EI 60</a>, of <a href="https://ontology.firebim.be/ontology/fbo#FireResistanceRating">REI 60</a> wanneer ze dragend zijn.
In deze <a href="https://ontology.firebim.be/ontology/fbo#Wall">wanden</a> mag een verbinding tussen deze <a href="https://ontology.firebim.be/ontology/fbo#Building">gebouwen</a> bestaan via een <a href="https://ontology.firebim.be/ontology/fbo#Door">deur</a> <a href="https://ontology.firebim.be/ontology/fbo#FireResistanceRating">EI1 30</a>, zelfsluitend of zelfsluitend in geval van <a href="https://ontology.firebim.be/ontology/fbo#Fire">brand</a>."""@nl .

fbd:Member_1_3_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:DistanceBetweenBuildings ;
    fbd:hasOriginalText "De voorwaarde van de <a href=\"https://ontology.firebim.be/ontology/fbo#DistanceBetweenBuildings\">afstand</a> tussen een laag <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> en een tegenoverstaand <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> geldt niet voor <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouwen</a> die van elkaar gescheiden worden door bestaande straten, wegen,... behorende tot het openbaar domein."@nl .

fbd:Member_2_1_1_1 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_5_2 ;
    fbd:hasKeyword fbd:Building,
        fbd:Compartment,
        fbd:CompartmentArea,
        fbd:Parking ;
    fbd:hasOriginalText "Het <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a> is verdeeld in <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> waarvan de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentArea\">oppervlakte</a> kleiner is dan 2500 m², met uitzondering van de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parkings</a> (zie 5.2)."@nl .

fbd:Member_2_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Compartment,
        fbd:CompartmentArea ;
    fbd:hasOriginalText "De maximale <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentArea\">oppervlakte</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gelijkvloers gebouw</a> bestaande uit één <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> mag 3500 m² bedragen."@nl .

fbd:Member_2_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Length ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Length\">lengte</a> van dit <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> bedraagt niet meer dan 90 m."@nl .

fbd:Member_2_1_1_4 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:CompartmentArea,
        fbd:HasAutomaticFireExtinguishingSystem,
        fbd:HasSmokeAndHeatExhaustSystem ;
    fbd:hasMember fbd:Member_2_1_1_4_1 ;
    fbd:hasOriginalText "De maximale <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentArea\">oppervlakte</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> mag, respectievelijk, groter dan ofwel 2500 m², ofwel 3500 m² bedragen indien het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> is uitgerust met een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> en een <a href=\"https://ontology.firebim.be/ontology/fbo#HasSmokeAndHeatExhaustSystem\">rook- en warmteafvoerinstallatie</a>."@nl .

fbd:Member_2_1_1_4_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:HasAutomaticFireExtinguishingSystem,
        fbd:HasSmokeAndHeatExhaustSystem ;
    fbd:hasOriginalText "De Minister van Binnenlandse Zaken bepaalt de voorwaarden waaronder uitzonderingen mogelijk zijn op de grootte van het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> respectievelijk ofwel 2500 m² ofwel 3500 m² zonder dat een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> en/of een <a href=\"https://ontology.firebim.be/ontology/fbo#HasSmokeAndHeatExhaustSystem\">rook- en warmteafvoerinstallatie</a> moet voorzien worden."@nl .

fbd:Member_2_1_2_1 a fbd:Member ;
    fbd:hasKeyword fbd:BuildingHeight,
        fbd:CompartmentHeight ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentHeight\">hoogte van een compartiment</a> stemt overeen met de <a href=\"https://ontology.firebim.be/ontology/fbo#BuildingHeight\">hoogte van één bouwlaag</a>."@nl .

fbd:Member_2_1_2_2 a fbd:Member ;
    fbd:hasMember fbd:Member_2_1_2_2_1,
        fbd:Member_2_1_2_2_2,
        fbd:Member_2_1_2_2_3,
        fbd:Member_2_1_2_2_4,
        fbd:Member_2_1_2_2_5 ;
    fbd:hasOriginalText "De volgende uitzonderingen zijn nochtans toegestaan:"@nl .

fbd:Member_2_1_2_2_1 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_5_2 ;
    fbd:hasKeyword fbd:Floor,
        fbd:Parking ;
    fbd:hasOriginalText "a)   de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a> met <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlagen</a> (zie 5.2);"@nl .

fbd:Member_2_1_2_2_2 a fbd:Member ;
    fbd:hasKeyword fbd:AdjacentFloors,
        fbd:Compartment,
        fbd:LevelArea,
        fbd:Staircase ;
    fbd:hasOriginalText "b)   een <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> mag zich uitstrekken over twee <a href=\"https://ontology.firebim.be/ontology/fbo#AdjacentFloors\">boven elkaar gelegen bouwlagen</a> met een <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">binnenverbindingstrap</a> (duplex), indien de gecumuleerde <a href=\"https://ontology.firebim.be/ontology/fbo#LevelArea\">oppervlakte van die bouwlagen</a> niet groter is dan 2500 m²;"@nl .

fbd:Member_2_1_2_2_3 a fbd:Member ;
    fbd:hasKeyword fbd:AdjacentFloors,
        fbd:Compartment,
        fbd:CompartmentHeight,
        fbd:FireAlarmDevicePresent,
        fbd:HasAutomaticFireDetectionSystem,
        fbd:LevelArea,
        fbd:SmokeDetector,
        fbd:Staircase ;
    fbd:hasOriginalText "c)   de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentHeight\">hoogte van een compartiment</a> mag zich uitstrekken over drie <a href=\"https://ontology.firebim.be/ontology/fbo#AdjacentFloors\">boven elkaar gelegen bouwlagen</a> met een <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">binnenverbindingstrap</a> (triplex), voor zover de som van hun gecumuleerde <a href=\"https://ontology.firebim.be/ontology/fbo#LevelArea\">oppervlakte</a> de 300 m² niet overschrijdt, en dat dit <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> is uitgerust met een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireDetectionSystem\">automatische branddetectie</a> van het type totale bewaking die automatisch een aanduiding van de <a href=\"https://ontology.firebim.be/ontology/fbo#FireAlarmDevicePresent\">brandmelding</a> geeft en waarvan de <a href=\"https://ontology.firebim.be/ontology/fbo#SmokeDetector\">detectoren</a> aangepast zijn aan de aanwezige risico's;"@nl .

fbd:Member_2_1_2_2_4 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_5_1_1 ;
    fbd:hasKeyword fbd:AdjacentFloors,
        fbd:Compartment,
        fbd:CompartmentHeight,
        fbd:TechnicalRoom ;
    fbd:hasOriginalText "d)   de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentHeight\">hoogte van een compartiment</a> mag zich uitstrekken over <a href=\"https://ontology.firebim.be/ontology/fbo#AdjacentFloors\">verscheidene boven elkaar geplaatste bouwlagen</a>, indien dit <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> slechts <a href=\"https://ontology.firebim.be/ontology/fbo#TechnicalRoom\">technische lokalen</a> omvat (zie 5.1.1)."@nl .

fbd:Member_2_1_2_2_5 a fbd:Member ;
    fbd:hasKeyword fbd:CompartmentHeight,
        fbd:Floor,
        fbd:IsAtrium ;
    fbd:hasMember fbd:Member_2_1_2_2_5_1 ;
    fbd:hasOriginalText "e)   de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentHeight\">hoogte van een compartiment</a> mag zich uitstrekken over verscheidene <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlagen</a> (<a href=\"https://ontology.firebim.be/ontology/fbo#IsAtrium\">atrium</a>) op voorwaarde:"@nl .

fbd:Member_2_1_2_2_5_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:HasAutomaticFireExtinguishingSystem,
        fbd:HasSmokeAndHeatExhaustSystem ;
    fbd:hasOriginalText "-    dat dit <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> is uitgerust met een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> en een <a href=\"https://ontology.firebim.be/ontology/fbo#HasSmokeAndHeatExhaustSystem\">rook- en warmteafvoerinstallatie</a>. De Minister van Binnenlandse Zaken bepaalt de voorwaarden waaronder uitzonderingen mogelijk zijn op de verplichte plaatsing van een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> en een <a href=\"https://ontology.firebim.be/ontology/fbo#HasSmokeAndHeatExhaustSystem\">rook- en warmteafvoerinstallatie</a>;"@nl .

fbd:Member_2_2_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment ;
    fbd:hasMember fbd:Member_2_2_1_1_1_1,
        fbd:Member_2_2_1_1_1_2,
        fbd:Member_2_2_1_1_1_3 ;
    fbd:hasOriginalText "Elk <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> heeft minimum:"@nl .

fbd:Member_2_2_1_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Exits,
        fbd:MaximumOccupancy ;
    fbd:hasOriginalText "-    één <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgang</a> indien de <a href=\"https://ontology.firebim.be/ontology/fbo#MaximumOccupancy\">maximale bezetting</a> minder dan 100 personen bedraagt;"@nl .

fbd:Member_2_2_1_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Exits,
        fbd:Occupancy ;
    fbd:hasOriginalText "-    twee <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> indien de <a href=\"https://ontology.firebim.be/ontology/fbo#Occupancy\">bezetting</a> 100 of meer dan 100 en minder dan 500 personen bedraagt;"@nl .

fbd:Member_2_2_1_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Exits,
        fbd:MaximumOccupancy,
        fbd:Occupancy ;
    fbd:hasOriginalText "-    2 + n <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> waarbij n het geheel getal is onmiddellijk groter dan het quotiënt van de deling door 1000 van de <a href=\"https://ontology.firebim.be/ontology/fbo#MaximumOccupancy\">maximale bezetting</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a>, indien de <a href=\"https://ontology.firebim.be/ontology/fbo#Occupancy\">bezetting</a> 500 of meer dan 500 personen bedraagt."@nl .

fbd:Member_2_2_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Exits,
        fbd:FireDepartmentApproval,
        fbd:Occupancy,
        fbd:Room ;
    fbd:hasOriginalText "Het minimum aantal <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> kan door de <a href=\"https://ontology.firebim.be/ontology/fbo#FireDepartmentApproval\">brandweer</a> verhoogd worden in functie van de <a href=\"https://ontology.firebim.be/ontology/fbo#Occupancy\">bezetting</a> en de configuratie van de <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a>."@nl .

fbd:Member_2_2_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Exits,
        fbd:Floor,
        fbd:Room ;
    fbd:hasOriginalText "Het aantal <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> van <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlagen</a> en <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> wordt bepaald zoals voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a>."@nl .

fbd:Member_2_2_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Exits ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> zijn gelegen in tegenovergestelde zones van het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a>."@nl .

fbd:Member_2_2_2_1_2 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_4_4 ;
    fbd:hasKeyword fbd:EvacuationRoute ;
    fbd:hasMember fbd:Member_2_2_2_1_2_1,
        fbd:Member_2_2_2_1_2_2,
        fbd:Member_2_2_2_1_2_3 ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> leiden ofwel:"@nl .

fbd:Member_2_2_2_1_2_1 a fbd:Member ;
    fbd:hasOriginalText "naar buiten;"@nl .

fbd:Member_2_2_2_1_2_2 a fbd:Member ;
    fbd:hasKeyword fbd:Staircase ;
    fbd:hasOriginalText "naar <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuizen</a>;"@nl .

fbd:Member_2_2_2_1_2_3 a fbd:Member ;
    fbd:hasKeyword fbd:DistanceBetweenBuildings,
        fbd:Staircase ;
    fbd:hasOriginalText "naar <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappen</a>, binnen of buiten het gebouw gelegen (voor de <a href=\"https://ontology.firebim.be/ontology/fbo#DistanceBetweenBuildings\">horizontale afstanden</a> zie 4.4)."@nl .

fbd:Member_2_2_2_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Door,
        fbd:EvacuationRoute,
        fbd:Exits,
        fbd:Staircase,
        fbd:UndergroundFloor,
        fbd:Wall ;
    fbd:hasOriginalText "Wat de <a href=\"https://ontology.firebim.be/ontology/fbo#UndergroundFloor\">ondergrondse bouwlagen</a> betreft mag één <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgang</a> naar buiten via een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatieweg</a> met <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> EI 30 en <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a> EI1 30 de vereiste toegang tot één der <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuizen</a> vervangen."@nl .

fbd:Member_2_2_2_1_4 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_5_2 ;
    fbd:hasKeyword fbd:Parking ;
    fbd:hasOriginalText "Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Parking\">parking</a>: zie 5.2."@nl .

fbd:Member_2_2_2_1_5 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_4_4 ;
    fbd:hasKeyword fbd:EvacuationLevel,
        fbd:EvacuationRoute,
        fbd:Staircase ;
    fbd:hasOriginalText "Op een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationLevel\">evacuatieniveau</a> leidt iedere <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trap</a> naar buiten, hetzij rechtstreeks, hetzij over een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatieweg</a> die beantwoordt aan de voorschriften van 4.4."@nl .

fbd:Member_3_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:ElectricalConduit,
        fbd:ExpansionJoint,
        fbd:FireResistance,
        fbd:Pipe,
        fbd:StructuralElement,
        fbd:Wall ;
    fbd:hasOriginalText "Doorvoeringen doorheen <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> van <a href=\"https://ontology.firebim.be/ontology/fbo#Pipe\">leidingen</a> voor fluïda of voor <a href=\"https://ontology.firebim.be/ontology/fbo#ElectricalConduit\">elektriciteit</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#ExpansionJoint\">uitzetvoegen</a> van <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> mogen de vereiste <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">bouwelementen</a> niet nadelig beïnvloeden."@nl .

fbd:Member_3_1_1_2 a fbd:Member ;
    fbd:hasOriginalText "De bepalingen van bijlage 7 “Gemeenschappelijke bepalingen”, hoofdstuk 1, zijn van toepassing."@nl ;
    fbd:hasReference fbd:Reference_Bijlage_7 .

fbd:Member_3_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationLevel,
        fbd:FireResistance,
        fbd:StructuralElement,
        fbd:Table ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">structurele elementen</a> beschikken, in functie van hun situatie, over een <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> zoals weergegeven in <a href=\"https://ontology.firebim.be/ontology/fbo#Table\">tabel</a> 2.1, waarin Ei het laagst gelegen <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationLevel\">evacuatieniveau</a> voorstelt."@nl .

fbd:Member_3_3_1_1 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_4_1,
        fbd:Section_4_4 ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Door,
        fbd:EvacuationRoute,
        fbd:Wall ;
    fbd:hasOriginalText "Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a>, die <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> afbakenen, geldt 4.1; bakenen zij <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a> af dan geldt 4.4."@nl .

fbd:Member_3_3_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:FireResistance,
        fbd:InnerWall,
        fbd:NightOccupancy,
        fbd:Room ;
    fbd:hasOriginalText "De verticale <a href=\"https://ontology.firebim.be/ontology/fbo#InnerWall\">binnenwanden</a> die <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> of het geheel van <a href=\"https://ontology.firebim.be/ontology/fbo#Room\">lokalen</a> met <a href=\"https://ontology.firebim.be/ontology/fbo#NightOccupancy\">nachtbezetting</a> afbakenen hebben een <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a>, aangegeven in tabel 2.2."@nl .

fbd:Member_3_3_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Door,
        fbd:Wall ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deuren</a> in deze <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> hebben EI1 30."@nl .

fbd:Member_3_4_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationRoute,
        fbd:FireStability,
        fbd:SuspendedCeiling ;
    fbd:hasOriginalText "In de <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatiewegen</a>, de voor het publiek toegankelijke lokalen en de collectieve keukens hebben de <a href=\"https://ontology.firebim.be/ontology/fbo#SuspendedCeiling\">verlaagde plafonds</a> EI 30 (a b), EI 30 (b a) of EI 30 (a ↔ b) volgens NBN EN 13501-2 en NBN EN 1364-2 of hebben een <a href=\"https://ontology.firebim.be/ontology/fbo#FireStability\">stabiliteit bij brand</a> van een ½ h volgens NBN 713-020."@nl ;
    fbd:hasReference fbd:Reference_NBN_713_020,
        fbd:Reference_NBN_EN_13501_2,
        fbd:Reference_NBN_EN_1364_2 .

fbd:Member_3_4_1_1_2 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_4_4_1_2 ;
    fbd:hasKeyword fbd:AutomaticFireExtinguishingSystem,
        fbd:Compartment ;
    fbd:hasOriginalText "Deze vereiste is niet van toepassing op de uitzonderingen opgenomen in punt 4.4.1.2 en op de <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> die zijn uitgerust met een <a href=\"https://ontology.firebim.be/ontology/fbo#AutomaticFireExtinguishingSystem\">automatische blusinstallatie</a> van het type sprinkler die is aangepast aan de aanwezige risico's."@nl .

fbd:Member_3_4_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Ceiling,
        fbd:FireResistance,
        fbd:SuspendedCeiling,
        fbd:Wall ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> waarvoor <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandwerendheid</a> vereist is, lopen door in de ruimte tussen het <a href=\"https://ontology.firebim.be/ontology/fbo#Ceiling\">plafond</a> en het <a href=\"https://ontology.firebim.be/ontology/fbo#SuspendedCeiling\">verlaagd plafond</a>."@nl .

fbd:Member_3_5_1_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Compartment,
        fbd:CurtainWall,
        fbd:FireResistanceRating,
        fbd:FireSpreadPrevention,
        fbd:Floor,
        fbd:HasAutomaticFireExtinguishingSystem,
        fbd:StructuralElement ;
    fbd:hasOriginalText "De stijlen van het <a href=\"https://ontology.firebim.be/ontology/fbo#CurtainWall\">gordijngevelskelet</a> worden ter hoogte van elke <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> aan het <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">gebouwskelet</a> bevestigd. Met uitzondering van de <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouwen</a> die uitgerust zijn met een <a href=\"https://ontology.firebim.be/ontology/fbo#HasAutomaticFireExtinguishingSystem\">automatische blusinstallatie</a>, moeten deze bevestigingen <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">R 60</a> zijn ten opzichte van een <a href=\"https://ontology.firebim.be/ontology/fbo#FireSpreadPrevention\">brand</a> in een onderliggend en naastgelegen <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a>."@nl .

fbd:Member_3_5_1_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:CompartmentBoundary,
        fbd:Facade,
        fbd:LinearJointSeal ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#LinearJointSeal\">lineaire voeg</a> aan de <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a> is dichtgemaakt zodat geen koude rook tussen de <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentBoundary\">compartimentswanden</a> kan dringen."@nl .

fbd:Member_3_5_1_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:CompartmentBoundary,
        fbd:Facade,
        fbd:FireResistanceRating,
        fbd:LinearJointSeal ;
    fbd:hasOriginalText "Bovendien heeft de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentBoundary\">aansluiting van de compartimentswanden</a> met de <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a>, met uitzondering van een beperkte <a href=\"https://ontology.firebim.be/ontology/fbo#LinearJointSeal\">lineaire voeg</a> met een breedte van maximaal 20 mm aan de <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a>, minstens <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI 60</a> of <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">EI 60</a> (i→o)."@nl .

fbd:Member_3_5_1_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:BuildingHeight,
        fbd:DistanceBetweenBuildings,
        fbd:Facade ;
    fbd:hasOriginalText """a) ofwel, als <a href="https://ontology.firebim.be/ontology/fbo#Facade">gevels</a> tegenover elkaar staan of een inspringende tweevlakshoek vormen, dan bedraagt de <a href="https://ontology.firebim.be/ontology/fbo#DistanceBetweenBuildings">afstand</a> (in m) tussen de <a href="https://ontology.firebim.be/ontology/fbo#Facade">geveldelen</a> die niet minstens E 30 of E 30 (o→i) hebben, ten minste:

- h + 10cos α voor 0° ≤ α ≤ 90°
- 2,5
- 0 voor 90° < α ≤ 180°

waarbij α de ingesloten hoek is en h de <a href="https://ontology.firebim.be/ontology/fbo#BuildingHeight">hoogte van het gebouw</a> in m (zie plaat 2.2)."""@nl .

fbd:Member_3_5_2_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:CompartmentBoundary,
        fbd:FireDamper,
        fbd:FireResistance ;
    fbd:hasOriginalText "Dit element mag openingen bevatten, op voorwaarde dat de continuïteit van de <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimentering</a> door de spouw heen verzekerd wordt door een bij brand zelfsluitende <a href=\"https://ontology.firebim.be/ontology/fbo#FireDamper\">afsluitinrichting</a> met een <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> E 60. Deze inrichting wordt beproefd in zijn dragende constructie, volgens de richting van de <a href=\"https://ontology.firebim.be/ontology/fbo#CompartmentBoundary\">compartimentswand</a>; de sluiting ervan wordt bevolen:"@nl .

fbd:Member_3_5_2_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Facade,
        fbd:SmokeDetector ;
    fbd:hasOriginalText """- hetzij door een thermische detectie ter hoogte van deze inrichting die reageert bij een temperatuur van maximaal 100°C.
- hetzij door een <a href="https://ontology.firebim.be/ontology/fbo#SmokeDetector">rookdetectie</a> in de spouw of in alle <a href="https://ontology.firebim.be/ontology/fbo#Compartment">compartimenten</a> langs de <a href="https://ontology.firebim.be/ontology/fbo#Facade">gevel</a>, die voldoet aan de voorwaarden in punt 3.5.2.3."""@nl .

fbd:Member_3_5_2_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Building,
        fbd:Compartment,
        fbd:Facade,
        fbd:FacadeVentilationOpening,
        fbd:SmokeDetector ;
    fbd:hasOriginalText "Wanneer er <a href=\"https://ontology.firebim.be/ontology/fbo#FacadeVentilationOpening\">verluchtingsopeningen</a> zijn tussen de spouw van de dubbelwandige <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a> en de binnenomgeving van het <a href=\"https://ontology.firebim.be/ontology/fbo#Building\">gebouw</a>, is enkel een <a href=\"https://ontology.firebim.be/ontology/fbo#SmokeDetector\">rookdetectie</a> toegelaten in de spouw of in alle <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> langs de <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevel</a>. Zij voldoet aan de voorwaarden van punt 3.5.2.3."@nl .

fbd:Member_3_5_2_2_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Facade,
        fbd:VentilationOpening ;
    fbd:hasOriginalText "De voorschriften voor enkelwandige <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevels</a> mogen toegepast worden op de binnenwand wanneer de buitenwand vaste of mobiele <a href=\"https://ontology.firebim.be/ontology/fbo#VentilationOpening\">ventilatieopeningen</a> bevat die automatisch openen bij brand."@nl .

fbd:Member_3_5_2_2_2_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:VentilationOpening ;
    fbd:hasOriginalText "De vaste <a href=\"https://ontology.firebim.be/ontology/fbo#VentilationOpening\">ventilatieopeningen</a> zijn geplaatst op 30 ± 10 graden naar de buitenkant en naar boven toe ten opzichte van de horizontale, gelijkmatig verdeeld over ten minste 50% van de oppervlakte ervan."@nl .

fbd:Member_3_5_2_2_2_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:VentilationOpening ;
    fbd:hasOriginalText "De mobiele <a href=\"https://ontology.firebim.be/ontology/fbo#VentilationOpening\">ventilatieopeningen</a> voldoen, bij brand, aan dezelfde voorwaarden als de vaste <a href=\"https://ontology.firebim.be/ontology/fbo#VentilationOpening\">ventilatieopeningen</a>."@nl .

fbd:Member_3_5_2_2_2_1_4 a fbd:Member ;
    fbd:hasKeyword fbd:AutomaticFireDetectionSystem,
        fbd:Compartment,
        fbd:Facade ;
    fbd:hasOriginalText "De veiligheidsstand van de mobiele lamellen wordt in werking gesteld door een algemene <a href=\"https://ontology.firebim.be/ontology/fbo#AutomaticFireDetectionSystem\">branddetectie-installatie</a> in de <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> langs <a href=\"https://ontology.firebim.be/ontology/fbo#Facade\">gevels</a>. De automatische bediening moet voldoen aan de voorwaarden voorzien in punt 3.5.2.3."@nl .

fbd:Member_3_5_2_3_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:AutomaticFireDetectionSystem ;
    fbd:hasOriginalText "De sluiting / opening wordt bevolen door een <a href=\"https://ontology.firebim.be/ontology/fbo#AutomaticFireDetectionSystem\">automatische branddetectie-installatie</a>."@nl .

fbd:Member_3_5_2_3_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:FirefighterAction,
        fbd:ManualControlSystem ;
    fbd:hasOriginalText "De installatie wordt uitgerust met <a href=\"https://ontology.firebim.be/ontology/fbo#ManualControlSystem\">manuele openings- en sluitingssystemen</a>. De bediening ervan is voorbehouden aan de <a href=\"https://ontology.firebim.be/ontology/fbo#FirefighterAction\">brandweer</a>. De plaats ervan moet bepaald worden in akkoord met de brandweer."@nl .

fbd:Member_3_5_2_3_3_1_1 a fbd:Member ;
    fbd:hasForwardReference fbd:Section_6_5_2 ;
    fbd:hasKeyword fbd:ElectricalConduit ;
    fbd:hasOriginalText "Wanneer de sluitings- / openingssystemen geen positieve veiligheid hebben, dan moeten de <a href=\"https://ontology.firebim.be/ontology/fbo#ElectricalConduit\">elektrische leidingen</a> die het sluitings-/openingssysteem verbinden, voldoen aan punt 6.5.2."@nl .

fbd:Member_4_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:FireResistance,
        fbd:SeparationWall ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#SeparationWall\">wanden tussen compartimenten</a> hebben minstens de <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> aangegeven in tabel 2.3."@nl .

fbd:Member_4_1_2_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Door,
        fbd:IsFireActivatedClosing,
        fbd:IsSelfClosing ;
    fbd:hasOriginalText "De verbinding tussen twee <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> is slechts toegestaan indien zij geschiedt via een <a href=\"https://ontology.firebim.be/ontology/fbo#IsSelfClosing\">zelfsluitende</a> of <a href=\"https://ontology.firebim.be/ontology/fbo#IsFireActivatedClosing\">bij brand zelfsluitende</a> <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deur</a> EI1 30."@nl .

fbd:Member_4_2_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Staircase ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappen</a> die verscheidene <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartimenten</a> verbinden zijn omsloten."@nl .

fbd:Member_4_2_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:EvacuationSafety ;
    fbd:hasOriginalText "De grondbeginselen van \"<a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">Compartimentering</a> en <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationSafety\">evacuatie</a>\" zijn erop van toepassing."@nl .

fbd:Member_4_2_2_3_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Door,
        fbd:Floor,
        fbd:Staircase ;
    fbd:hasOriginalText "Op iedere <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> wordt de verbinding tussen het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> en het <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuis</a> verzekerd door een <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deur</a> EI1 30."@nl .

fbd:Member_4_2_2_3_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Floor,
        fbd:IsDuplex,
        fbd:Staircase ;
    fbd:hasMember fbd:Member_4_2_2_3_1_2_1,
        fbd:Member_4_2_2_3_1_2_2 ;
    fbd:hasOriginalText "Een rechtstreekse verbinding van elke <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#IsDuplex\">duplex</a> met het <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuis</a> is niet vereist, op voorwaarde dat:"@nl .

fbd:Member_4_2_2_3_1_2_1 a fbd:Member ;
    fbd:hasKeyword fbd:DuplexCompartment,
        fbd:TotalArea ;
    fbd:hasOriginalText "- de totale <a href=\"https://ontology.firebim.be/ontology/fbo#TotalArea\">oppervlakte</a> van het <a href=\"https://ontology.firebim.be/ontology/fbo#DuplexCompartment\">duplexcompartiment</a> of het duplexappartement kleiner is dan of gelijk is aan 300 m²;"@nl .

fbd:Member_4_2_2_3_1_2_2 a fbd:Member ;
    fbd:hasKeyword fbd:Duplex,
        fbd:EvacuationRoute,
        fbd:Floor,
        fbd:LevelArea,
        fbd:Staircase ;
    fbd:hasOriginalText "- de <a href=\"https://ontology.firebim.be/ontology/fbo#LevelArea\">oppervlakte</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#Duplex\">duplex</a> die niet rechtstreeks <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacueert</a> via het <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuis</a>, kleiner is dan of gelijk is aan 150 m²."@nl .

fbd:Member_4_2_2_3_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Floor,
        fbd:IsTriplex,
        fbd:Staircase ;
    fbd:hasMember fbd:Member_4_2_2_3_1_3_1,
        fbd:Member_4_2_2_3_1_3_2 ;
    fbd:hasOriginalText "De rechtstreekse verbinding van elke <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> van een <a href=\"https://ontology.firebim.be/ontology/fbo#IsTriplex\">triplex</a> met het <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuis</a> is niet vereist, op voorwaarde dat:"@nl .

fbd:Member_4_2_2_3_1_3_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationRoute,
        fbd:Floor,
        fbd:LevelArea,
        fbd:Staircase,
        fbd:Triplex ;
    fbd:hasOriginalText "- de <a href=\"https://ontology.firebim.be/ontology/fbo#LevelArea\">oppervlakte</a> van elke <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#Triplex\">triplex</a> die niet rechtstreeks <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacueert</a> via het <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuis</a>, kleiner is dan of gelijk is aan 100 m²;"@nl .

fbd:Member_4_2_2_3_1_3_2 a fbd:Member ;
    fbd:hasKeyword fbd:Triplex ;
    fbd:hasOriginalText "- de verbinding met de <a href=\"https://ontology.firebim.be/ontology/fbo#Triplex\">triplex</a> minstens gebeurt:"@nl .

fbd:Member_4_2_2_5_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:FireResistance,
        fbd:Wall ;
    fbd:hasOriginalText "1. de <a href=\"https://ontology.firebim.be/ontology/fbo#Wall\">wanden</a> die ze scheiden hebben <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">EI 60</a>."@nl .

fbd:Member_4_2_2_5_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Door,
        fbd:FireResistance,
        fbd:IsFireActivatedClosing ;
    fbd:hasOriginalText "2. de toegang tot ieder trappenhuis geschiedt via een <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">zelfsluitende</a> of <a href=\"https://ontology.firebim.be/ontology/fbo#IsFireActivatedClosing\">bij brand zelfsluitende</a> <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deur</a> <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">EI1 30</a>."@nl .

fbd:Member_4_2_3_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:FireResistanceRating,
        fbd:FireStability,
        fbd:Landing,
        fbd:MaterialClass,
        fbd:Staircase,
        fbd:StructuralElement ;
    fbd:hasOriginalText "1. evenals de <a href=\"https://ontology.firebim.be/ontology/fbo#Landing\">overlopen</a> hebben zij <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">R 30</a> of zijn op dezelfde manier ontworpen als een <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">betonplaat</a> met <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistanceRating\">R 30</a>. Er wordt evenwel geen <a href=\"https://ontology.firebim.be/ontology/fbo#FireStability\">stabiliteit bij brand</a> vereist voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappen</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#Landing\">overlopen</a> die uitsluitend zijn samengesteld uit <a href=\"https://ontology.firebim.be/ontology/fbo#MaterialClass\">materialen van klasse A1</a>, met een smelttemperatuur groter dan 727°C (staal voldoet bijvoorbeeld aan deze voorwaarde, aluminium en glas voldoen niet aan deze voorwaarde);"@nl .

fbd:Member_4_2_3_1_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Handrail,
        fbd:Staircase,
        fbd:UsefulWidth ;
    fbd:hasOriginalText "2. zij zijn aan beide zijden uitgerust met <a href=\"https://ontology.firebim.be/ontology/fbo#Handrail\">leuningen</a>. Voor de <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappen</a> met een <a href=\"https://ontology.firebim.be/ontology/fbo#UsefulWidth\">nuttige breedte</a>, kleiner dan 1,20 m, is één <a href=\"https://ontology.firebim.be/ontology/fbo#Handrail\">leuning</a> voldoende, voor zover er geen gevaar is voor het vallen;"@nl .

fbd:Member_4_2_3_1_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:TreadDepth ;
    fbd:hasOriginalText "3. de <a href=\"https://ontology.firebim.be/ontology/fbo#TreadDepth\">aantrede van de treden</a> is in elk punt ten minste 20 cm;"@nl .

fbd:Member_4_2_3_1_1_4 a fbd:Member ;
    fbd:hasKeyword fbd:RiserHeight ;
    fbd:hasOriginalText "4. de <a href=\"https://ontology.firebim.be/ontology/fbo#RiserHeight\">optrede van de treden</a> mag niet meer dan 18 cm bedragen;"@nl .

fbd:Member_4_2_3_1_1_5 a fbd:Member ;
    fbd:hasKeyword fbd:MaximumSlope ;
    fbd:hasOriginalText "5. hun <a href=\"https://ontology.firebim.be/ontology/fbo#MaximumSlope\">helling</a> mag niet meer dan 75 % bedragen (maximale hellingshoek 37°);"@nl .

fbd:Member_4_2_3_1_1_6 a fbd:Member ;
    fbd:hasKeyword fbd:Tread,
        fbd:TreadDepth ;
    fbd:hasOriginalText "6. zij zijn van het \"rechte type\". Maar, \"wenteltrappen\" worden toegestaan zo ze verdreven <a href=\"https://ontology.firebim.be/ontology/fbo#Tread\">treden</a> hebben en zo hun <a href=\"https://ontology.firebim.be/ontology/fbo#Tread\">treden</a>, naast de hiervoor vermelde vereisten, (met uitzondering van voornoemd punt 3), ten minste 24 cm <a href=\"https://ontology.firebim.be/ontology/fbo#TreadDepth\">aantrede</a> hebben op de looplijn."@nl .

fbd:Member_4_2_3_2_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:RequiredUsefulWidth,
        fbd:UsefulWidth ;
    fbd:hasOriginalText "Deze <a href=\"https://ontology.firebim.be/ontology/fbo#UsefulWidth\">nuttige breedte</a> is ten minste gelijk aan 0,80 m en bereikt ten minste de vereiste <a href=\"https://ontology.firebim.be/ontology/fbo#RequiredUsefulWidth\">nuttige breedte</a> br berekend volgens bijlage 1 \"Terminologie\"."@nl .

fbd:Member_4_2_3_2_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:Landing,
        fbd:Staircase,
        fbd:UsefulWidth ;
    fbd:hasOriginalText "De <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">traparmen</a> en de <a href=\"https://ontology.firebim.be/ontology/fbo#Landing\">overlopen</a> der <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trappenhuizen</a> van één zelfde <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> mogen in hun <a href=\"https://ontology.firebim.be/ontology/fbo#UsefulWidth\">nuttige breedte</a> niet meer dan één doorgangseenheid verschillen."@nl .

fbd:Member_4_3_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationLevel,
        fbd:ExternalStaircase ;
    fbd:hasOriginalText "<a href=\"https://ontology.firebim.be/ontology/fbo#ExternalStaircase\">Buitentrappen</a> moeten toegang geven tot een <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationLevel\">evacuatieniveau</a>."@nl .

fbd:Member_4_3_1_2 a fbd:Member ;
    fbd:hasKeyword fbd:FireStability,
        fbd:Material ;
    fbd:hasOriginalText "De voorschriften van 4.2.3 zijn er op van toepassing met nochtans de volgende afwijking: er wordt geen <a href=\"https://ontology.firebim.be/ontology/fbo#FireStability\">stabiliteit bij brand</a> vereist, maar het <a href=\"https://ontology.firebim.be/ontology/fbo#Material\">materiaal</a> behoort tot klasse A1."@nl .

fbd:Member_4_3_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:ExternalStaircase ;
    fbd:hasMember fbd:Member_4_3_1_3_1,
        fbd:Member_4_3_1_3_2 ;
    fbd:hasOriginalText "De verbinding tussen het <a href=\"https://ontology.firebim.be/ontology/fbo#Compartment\">compartiment</a> en een <a href=\"https://ontology.firebim.be/ontology/fbo#ExternalStaircase\">buitentrap</a> gebeurt:"@nl .

fbd:Member_4_3_1_3_1 a fbd:Member ;
    fbd:hasKeyword fbd:Door ;
    fbd:hasOriginalText "- hetzij via een <a href=\"https://ontology.firebim.be/ontology/fbo#Door\">deur</a>;"@nl .

fbd:Member_4_3_1_3_2 a fbd:Member ;
    fbd:hasKeyword fbd:EscapeTerrace ;
    fbd:hasOriginalText "- hetzij via (een) <a href=\"https://ontology.firebim.be/ontology/fbo#EscapeTerrace\">vluchtterras</a>(sen)."@nl .

fbd:Member_4_3_1_4 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationLevel,
        fbd:Floor,
        fbd:Staircase ;
    fbd:hasOriginalText "Voor de verbinding tussen het <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationLevel\">evacuatieniveau</a> en de onmiddellijk hoger gelegen <a href=\"https://ontology.firebim.be/ontology/fbo#Floor\">bouwlaag</a> mag echter een <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trap</a> of gedeelte van <a href=\"https://ontology.firebim.be/ontology/fbo#Staircase\">trap</a> aangewend worden die inschuifbaar of geleed is."@nl .

fbd:Member_4_4_1_1_1_1 a fbd:Member ;
