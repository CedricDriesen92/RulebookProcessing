fbd:Member_4_4_1_1_1_1_1 a fbd:Member ;
    fbd:hasKeyword fbd:EvacuationRoute,
        fbd:Exits ;
    fbd:hasOriginalText "     -    30 m van de <a href=\"https://ontology.firebim.be/ontology/fbo#EvacuationRoute\">evacuatieweg</a> die de <a href=\"https://ontology.firebim.be/ontology/fbo#Exits\">uitgangen</a> verbindt;"@nl .

fbd:Member_4_4_1_1_1_1_2 a fbd:Member ;
