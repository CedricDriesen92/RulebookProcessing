    fbd:hasOriginalText "2. ter hoogte van de <a href=\"https://ontology.firebim.be/ontology/fbo#WallPenetration\">doorvoering</a> bevindt zich een <a href=\"https://ontology.firebim.be/ontology/fbo#StructuralElement\">bouwelement</a> met minstens de vereiste <a href=\"https://ontology.firebim.be/ontology/fbo#FireResistance\">brandweerstand</a> van de <a href=\"https://ontology.firebim.be/ontology/fbo#VerticalShaft\">verticale wand</a>;"@nl .

fbd:Member_5_1_5_2_1_3 a fbd:Member ;
    fbd:hasKeyword fbd:Compartment,
        fbd:FireResistanceRating,
        fbd:ShaftHatch,
        fbd:ShaftWall,
        fbd:VerticalShaft ;
