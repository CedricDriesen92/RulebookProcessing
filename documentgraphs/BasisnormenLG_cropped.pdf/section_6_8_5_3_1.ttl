firebim:Section_6_8_5_3_1 a firebim:Section ;
    firebim:hasID "6.8.5.3.1" ;
    firebim:hasOriginalText "HET AANTAL EN DE PLAATS VAN DEZE TOESTELLEN WORDEN BEPAALD DOOR DE AARD EN DE OMVANG VAN HET BRANDGEVAAR."@nl ;
    firebim:hasArticle firebim:Article_6_8_5_3_1_1 .

firebim:Article_6_8_5_3_1_1 a firebim:Article ;
    firebim:hasID "A6.8.5.3.1.1" ;
    firebim:hasMember firebim:Member_6_8_5_3_1_1_1,
        firebim:Member_6_8_5_3_1_1_2,
        firebim:Member_6_8_5_3_1_1_3 .

firebim:Member_6_8_5_3_1_1_1 a firebim:Member ;
    firebim:hasOriginalText """Indien de <a href="http://example.com/firebimbuilding#TotalArea">oppervlakte</a> van een <a href="http://example.com/firebimbuilding#Building">gebouw</a> kleiner is dan 500 m² is geen <a href="http://example.com/firebimbuilding#FireHoseReel">muurhaspel</a> verplicht (behoudens bij speciale risico’s)."""@nl .

firebim:Member_6_8_5_3_1_1_2 a firebim:Member ;
    firebim:hasOriginalText """In alle andere gevallen wordt het aantal <a href="http://example.com/firebimbuilding#FireHoseReel">muurhaspels</a> als volgt bepaald:"""@nl ;
    firebim:hasMember firebim:Member_6_8_5_3_1_1_2_1,
        firebim:Member_6_8_5_3_1_1_2_2 .

firebim:Member_6_8_5_3_1_1_2_1 a firebim:Member ;
    firebim:hasOriginalText "1. de waterstraal bereikt elk punt van een <a href=\"http://example.com/firebimbuilding#Compartment\">compartiment</a>;"@nl .

firebim:Member_6_8_5_3_1_1_2_2 a firebim:Member ;
    firebim:hasOriginalText "2. <a href=\"http://example.com/firebimbuilding#Compartment\">compartimenten</a> groter dan 500 m² beschikken over ten minste 1 <a href=\"http://example.com/firebimbuilding#FireHoseReel\">muurhaspel</a>."@nl .

firebim:Member_6_8_5_3_1_1_3 a firebim:Member ;
    firebim:hasOriginalText "Het perskoppelstuk van de eventuele <a href=\"http://example.com/firebimbuilding#FireHydrant\">muurhydranten</a> is aangepast aan de koppelingen gebruikt door de brandweer."@nl .