fro:Section_6_8_5_3_2 a fro:Section ;
    fro:hasID "6.8.5.3.2" ;
    fro:hasOriginalText "DE STIJGLEIDING DIE EVENTUELE TOESTELLEN VOEDT MET WATER ONDER DRUK, HEEFT DE VOLGENDE KENMERKEN:" .

fro:Section_6_8_5_3_3 a fro:Section ;
    fro:hasID "6.8.5.3.3" ;
    fro:hasOriginalText "DE EVENTUELE TOESTELLEN WORDEN ZONDER VOORAFGAANDE BEDIENING GEVOED MET WATER ONDER DRUK. DEZE DRUK BEDRAAGT TEN MINSTE 2,5 BAR OP HET ONGUNSTIGSTE PUNT." .

fro:Section_6_8_5_4 a fro:Section ;
    fro:hasID "6.8.5.4" ;
