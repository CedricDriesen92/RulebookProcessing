firebim:Section_6_4_3 a firebim:Section ;
    firebim:hasID "6.4.3" ;
    firebim:hasOriginalText "Afwijkende bepalingen."@nl ;
    firebim:hasArticle firebim:Article_6_4_3_1 .

firebim:Article_6_4_3_1 a firebim:Article ;
    firebim:hasID "A6.4.3.1" ;
    firebim:hasOriginalText """Voor de gebouwen waarvoor de aanvraag voor de bouw werd ingediend voor 1 april 2017, gelden de volgende afwijkende bepalingen:"""@nl ;
    firebim:hasMember firebim:Member_6_4_3_1_1,
        firebim:Member_6_4_3_1_2,
        firebim:Member_6_4_3_1_3 .

firebim:Member_6_4_3_1_1 a firebim:Member ;
    firebim:hasOriginalText """- Punt 6.4.1.4: De <a href=\"http://example.com/firebimbuilding#ShaftDoor\">schachtdeuren</a> van <a href=\"http://example.com/firebimbuilding#Elevator\">liften</a> ontworpen voor 1 april 2017, <a href=\"http://example.com/firebimbuilding#IsAutomaticClosing\">openen en sluiten automatisch</a> en hebben een <a href=\"http://example.com/firebimbuilding#UsefulWidth\">nuttige breedte</a> van ten minste 0,80 m."""@nl .

firebim:Member_6_4_3_1_2 a firebim:Member ;
    firebim:hasOriginalText """- Punten 6.4.1.6, 6.4.1.7 en 6.4.1.8: Enkel van toepassing op <a href=\"http://example.com/firebimbuilding#Elevator\">liften</a> die ontworpen of gemoderniseerd werden na 31 maart 2017."""@nl .

firebim:Member_6_4_3_1_3 a firebim:Member ;
    firebim:hasOriginalText """- Punten 6.4.2: Niet van toepassing."""@nl .