@prefix firebim: <http://example.com/firebim#> .
@base <http://example.com/firebim> .

firebim:Section_0_2_1 a firebim:Section ;
    firebim:hasID "0.2.1" ;
    firebim:hasOriginalText "DEZE BIJLAGE IS VAN TOEPASSING OP DE VOLGENDE OP TE RICHTEN <a href='http://example.com/firebim#Document'>GEBOUWEN</a> EN DE VOLGENDE UITBREIDINGEN VAN BESTAANDE <a href='http://example.com/firebim#Document'>GEBOUWEN</a>, WAARVOOR DE AANVRAAG VOOR DE BOUW WORDT INGEDIEND VANAF 1 DECEMBER 2012:"@nl ;
    firebim:hasArticle firebim:Article_0_2_1_1 .

firebim:Article_0_2_1_1 a firebim:Article ;
    firebim:hasID "A0.2.1.1" ;
    firebim:hasMember firebim:Member_0_2_1_1_1,
        firebim:Member_0_2_1_1_2,
        firebim:Member_0_2_1_1_3 .

firebim:Member_0_2_1_1_1 a firebim:Member ;
    firebim:hasOriginalText "1. de lage <a href='http://example.com/firebim#Document'>gebouwen</a>;"@nl .

firebim:Member_0_2_1_1_2 a firebim:Member ;
    firebim:hasOriginalText "2. de uitbreidingen van <a href='http://example.com/firebim#Document'>gebouwen</a> die na realisatie een laag <a href='http://example.com/firebim#Document'>gebouw</a> zijn;"@nl .

firebim:Member_0_2_1_1_3 a firebim:Member ;
    firebim:hasOriginalText "3. de lokalen of delen van lage <a href='http://example.com/firebim#Document'>gebouwen</a> waarin een industriële activiteit plaatsvindt en waarvan de totale <a href='http://example.com/firebim#Section'>oppervlakte</a> kleiner is dan of gelijk is aan 500 m², onder de volgende voorwaarden:"@nl ;
    firebim:hasMember firebim:Member_0_2_1_1_3_1,
        firebim:Member_0_2_1_1_3_2,
        firebim:Member_0_2_1_1_3_3 .

firebim:Member_0_2_1_1_3_1 a firebim:Member ;
    firebim:hasOriginalText "- in het <a href='http://example.com/firebim#Document'>gebouw</a> hoofdzakelijk niet-industriële activiteiten plaats vinden en de totale <a href='http://example.com/firebim#Section'>oppervlakte</a> van de lokalen met industriële activiteit kleiner is dan de overblijvende <a href='http://example.com/firebim#Section'>oppervlakte</a> van het <a href='http://example.com/firebim#Document'>gebouw</a>;"@nl .

firebim:Member_0_2_1_1_3_2 a firebim:Member ;
    firebim:hasOriginalText "- de industriële activiteiten in deze lokalen de niet-industriële activiteiten in hetzelfde <a href='http://example.com/firebim#Section'>compartiment</a> ondersteunen;"@nl .

firebim:Member_0_2_1_1_3_3 a firebim:Member ;
    firebim:hasOriginalText "- er geen lokalen met nachtbezetting zijn in het <a href='http://example.com/firebim#Section'>compartiment</a> waarin er industriële activiteiten plaatsvinden."@nl .