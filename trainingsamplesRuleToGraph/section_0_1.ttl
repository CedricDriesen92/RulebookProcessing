fro:Section_0_1 a fro:Section ;
    fro:hasID "0.1" ;
    fro:hasOriginalText "Doel."@nl ;
    fro:hasArticle fro:Article_0_1_1 .

fro:Article_0_1_1 a fro:Article ;
    fro:hasID "A0.1.1" ;
    fro:hasOriginalText """Deze basisreglementering bepaalt de minimale eisen waaraan de opvatting, de bouw en de
inrichting van lage gebouwen (LG) moeten voldoen om:"""@nl ;
    fro:hasMember fro:Member_0_1_1_1, fro:Member_0_1_1_2, fro:Member_0_1_1_3 .

fro:Member_0_1_1_1 a fro:Member ;
    fro:hasOriginalText "het ontstaan, de ontwikkeling en de voortplanting van brand te voorkomen;"@nl .

fro:Member_0_1_1_2 a fro:Member ;
    fro:hasOriginalText "de veiligheid van de aanwezigen te waarborgen;"@nl .

fro:Member_0_1_1_3 a fro:Member ;
    fro:hasOriginalText "preventief het ingrijpen van de brandweer te vergemakkelijken."@nl .