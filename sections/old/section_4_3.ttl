firebim:Article_4_3_3 a firebim:Article ;
    firebim:hasOriginalText """De verbinding tussen het compartiment en een buitentrap gebeurt                  :
-    hetzij via een deur;
-    hetzij via (een) vluchtterras(sen)."""@nl ;
    firebim:hasMember firebim:Member_4_3_3_1 .

firebim:Member_4_3_3_1 a firebim:Member ;
