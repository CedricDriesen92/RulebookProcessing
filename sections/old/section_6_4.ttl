firebim:Section_6_4 a firebim:Section ;
    firebim:hasID "6_4" ;
    firebim:hasOriginalText "Speciale liften."@nl ;
    firebim:hasArticle firebim:Article_6_4_1 .

firebim:Article_6_4_1 a firebim:Article ;
    firebim:hasOriginalText """Speciale liften.
De speciale liften en de werking ervan bij brand beantwoorden aan de volgende voorschriften of
            elke  andere  regel  van  goed  vakmanschap  die  een  gelijkwaardig  veiligheidsniveau  biedt,
            overeenkomstig het koninklijk besluit van 12 april 2016 betreffende het op de markt brengen van
            liften en veiligheidscomponenten voor liften."""@nl ;
    firebim:hasMember firebim:Member_6_4_1_1 .

firebim:Member_6_4_1_1 a firebim:Member ;
    firebim:hasOriginalText """De speciale liften en de werking ervan bij brand beantwoorden aan de volgende voorschriften of
