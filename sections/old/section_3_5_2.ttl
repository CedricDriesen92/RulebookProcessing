firebim:Article_3_5_2_2 a firebim:Article ;
    firebim:hasID "3.5.2.2" ;
    firebim:hasOriginalText """Dubbelwandige gevel zonder compartimentering.
            De dubbelwandige gevels zonder compartimentering moeten in overeenstemming zijn met een
            van de twee hierna opgenomen mogelijkheden."""@nl ;
    firebim:hasMember firebim:Member_3_5_2_2_1 ;
    firebim:hasSection firebim:Section_3_5_2_2_1, firebim:Section_3_5_2_2_2 .

