firebim:Section_6_4_3 a firebim:Section ;
    firebim:hasID "6_4_3" ;
    firebim:hasOriginalText """Afwijkende bepalingen."""@nl ;
    firebim:hasArticle firebim:Article_6_4_3_1 .

firebim:RoyalDecree firebim:hasSection firebim:Section_6_4_3 .

firebim:Article_6_4_3_1 a firebim:Article ;
    firebim:hasOriginalText """Voor de gebouwen waarvoor de aanvraag voor de bouw werd ingediend voor 1 april 2017, gelden
            de volgende afwijkende bepalingen:
            -    Punt 6.4.1.4: De schachtdeuren van liften ontworpen voor 1 april 2017, openen en sluiten
                 automatisch en hebben een nuttige breedte van ten minste 0,80 m.
            -    Punten  6.4.1.6,  6.4.1.7  en  6.4.1.8:  Enkel  van  toepassing  op  liften  die  ontworpen  of
                 gemoderniseerd werden na 31 maart 2017.
            -    Punten 6.4.2: Niet van toepassing."""@nl ;
    firebim:hasMember firebim:Member_6_4_3_1_1,
                      firebim:Member_6_4_3_1_2,
                      firebim:Member_6_4_3_1_3,
                      firebim:Member_6_4_3_1_4 .

