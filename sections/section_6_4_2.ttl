    firebim:hasID "6.4.2" ;
    firebim:hasOriginalText "Liften bestemd voor de brandweer."@nl ;
    firebim:hasArticle firebim:Article_6_4_2_1 ;
    firebim:hasSection firebim:Section_6_4_2_1, firebim:Section_6_4_2_2, firebim:Section_6_4_2_3, 
