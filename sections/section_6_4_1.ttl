    firebim:hasID "A6.4.1.6.1" ;
    firebim:hasOriginalText """De liften bestemd voor de evacuatie van personen met beperkte mobiliteit worden aangeduid met
een duidelijke en herkenbare signalering."""@nl ;
    firebim:hasMember firebim:Member_6_4_1_6_1_1 .

