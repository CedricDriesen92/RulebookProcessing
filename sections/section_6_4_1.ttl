    firebim:hasID "A6.4.1.1.1" ;
    firebim:hasOriginalText """Deze lift moet zodanig ontworpen en gebouwd worden dat de toegang en het gebruik door
personen met beperkte mobiliteit niet belemmerd of verhinderd wordt."""@nl ;
    firebim:hasMember firebim:Member_6_4_1_1_1_1 .

