firebim:Section_6_8_5 a firebim:Section ;
    firebim:hasID "6.8.5" ;
    firebim:hasOriginalText "Brandbestrijdingsmiddelen."@nl ;
    firebim:hasSection firebim:Section_6_8_5_1, firebim:Section_6_8_5_2, firebim:Section_6_8_5_3, firebim:Section_6_8_5_4 .

firebim:Section_6_8 firebim:hasSection firebim:Section_6_8_5 .

firebim:Section_6_8_5_1 a firebim:Section ;
    firebim:hasID "6.8.5.1" ;
    firebim:hasOriginalText "Algemeen."@nl ;
    firebim:hasArticle firebim:Article_6_8_5_1_1 .

firebim:Article_6_8_5_1_1 a firebim:Article ;
    firebim:hasID "A6.8.5.1.1" ;
    firebim:hasMember firebim:Member_6_8_5_1_1_1, firebim:Member_6_8_5_1_1_2 .

firebim:Member_6_8_5_1_1_1 a firebim:Member ;
    firebim:hasOriginalText "De brandbestrijdingsmiddelen bestaan uit toestellen of installaties die al dan niet automatisch zijn."@nl .

firebim:Member_6_8_5_1_1_2 a firebim:Member ;
    firebim:hasOriginalText """De snelblussers en de muurhaspels dienen voor eerste interventie, dat wil zeggen dat zij bestemd
zijn voor gebruik door bewoners."""@nl .

firebim:Section_6_8_5_2 a firebim:Section ;
    firebim:hasID "6.8.5.2" ;
    firebim:hasOriginalText "Draagbare of mobiele snelblussers."@nl ;
    firebim:hasArticle firebim:Article_6_8_5_2_1 .

firebim:Article_6_8_5_2_1 a firebim:Article ;
    firebim:hasID "A6.8.5.2.1" ;
    firebim:hasMember firebim:Member_6_8_5_2_1_1 .

firebim:Member_6_8_5_2_1_1 a firebim:Member ;
    firebim:hasOriginalText "Deze toestellen worden bepaald door de aard en de omvang van het gevaar."@nl .

firebim:Section_6_8_5_3 a firebim:Section ;
    firebim:hasID "6.8.5.3" ;
    firebim:hasOriginalText "Muurhaspels met axiale voeding, muurhydranten."@nl ;
    firebim:hasSection firebim:Section_6_8_5_3_1, firebim:Section_6_8_5_3_2, firebim:Section_6_8_5_3_3 .

firebim:Section_6_8_5_3_1 a firebim:Section ;
    firebim:hasID "6.8.5.3.1" ;
    firebim:hasArticle firebim:Article_6_8_5_3_1_1 .

firebim:Article_6_8_5_3_1_1 a firebim:Article ;
    firebim:hasID "A6.8.5.3.1.1" ;
    firebim:hasMember firebim:Member_6_8_5_3_1_1_1, firebim:Member_6_8_5_3_1_1_2, firebim:Member_6_8_5_3_1_1_3 .

firebim:Member_6_8_5_3_1_1_1 a firebim:Member ;
    firebim:hasOriginalText "Het aantal en de plaats van deze toestellen worden bepaald door de aard en de omvang van het brandgevaar."@nl .

firebim:Member_6_8_5_3_1_1_2 a firebim:Member ;
    firebim:hasOriginalText """Indien  de  oppervlakte  van  een  gebouw  kleiner  is  dan  500 m²  is  geen  muurhaspel  verplicht
(behoudens bij speciale risico's). In alle andere gevallen wordt het aantal muurhaspels als volgt
bepaald:"""@nl ;
    firebim:hasMember firebim:Member_6_8_5_3_1_1_2_1, firebim:Member_6_8_5_3_1_1_2_2 .

firebim:Member_6_8_5_3_1_1_2_1 a firebim:Member ;
    firebim:hasOriginalText "1.   de waterstraal bereikt elk punt van een compartiment;"@nl .

firebim:Member_6_8_5_3_1_1_2_2 a firebim:Member ;
    firebim:hasOriginalText "2.   compartimenten groter dan 500 m² beschikken over ten minste 1 muurhaspel."@nl .

firebim:Member_6_8_5_3_1_1_3 a firebim:Member ;
    firebim:hasOriginalText """Het perskoppelstuk van de eventuele muurhydranten is aangepast aan de koppelingen gebruikt
door de brandweer."""@nl .

firebim:Section_6_8_5_3_2 a firebim:Section ;
    firebim:hasID "6.8.5.3.2" ;
    firebim:hasArticle firebim:Article_6_8_5_3_2_1 .

firebim:Article_6_8_5_3_2_1 a firebim:Article ;
    firebim:hasID "A6.8.5.3.2.1" ;
    firebim:hasMember firebim:Member_6_8_5_3_2_1_1 .

firebim:Member_6_8_5_3_2_1_1 a firebim:Member ;
    firebim:hasOriginalText """De stijgleiding die eventuele toestellen voedt met water onder druk, heeft de volgende kenmerken:
de binnendiameter en de voedingsdruk moeten zodanig zijn dat de druk aan de minst bedeelde
haspel beantwoordt aan de voorschriften van NBN EN 671-1, ermee rekening houdend dat 3
haspels met axiale voeding gelijktijdig moeten kunnen werken gedurende ½ h."""@nl .

firebim:Member_6_8_5_3_2_1_1 firebim:hasReference firebim:Reference_NBN_EN_671_1 .

firebim:Reference_NBN_EN_671_1 a firebim:Reference ;
    firebim:hasID "NBN EN 671-1" .

firebim:Section_6_8_5_3_3 a firebim:Section ;
    firebim:hasID "6.8.5.3.3" ;
    firebim:hasArticle firebim:Article_6_8_5_3_3_1 .

firebim:Article_6_8_5_3_3_1 a firebim:Article ;
    firebim:hasID "A6.8.5.3.3.1" ;
    firebim:hasMember firebim:Member_6_8_5_3_3_1_1, firebim:Member_6_8_5_3_3_1_2 .

firebim:Member_6_8_5_3_3_1_1 a firebim:Member ;
    firebim:hasOriginalText "De eventuele toestellen worden zonder voorafgaande bediening gevoed met water onder druk."@nl .

firebim:Member_6_8_5_3_3_1_2 a firebim:Member ;
    firebim:hasOriginalText "Deze druk bedraagt ten minste 2,5 bar op het ongunstigste punt."@nl .

firebim:Section_6_8_5_4 a firebim:Section ;
    firebim:hasID "6.8.5.4" ;
    firebim:hasOriginalText "Ondergrondse en bovengrondse hydranten."@nl ;
    firebim:hasSection firebim:Section_6_8_5_4_1, firebim:Section_6_8_5_4_2, firebim:Section_6_8_5_4_3 .

firebim:Section_6_8_5_4_1 a firebim:Section ;
    firebim:hasID "6.8.5.4.1" ;
    firebim:hasArticle firebim:Article_6_8_5_4_1_1 .

firebim:Article_6_8_5_4_1_1 a firebim:Article ;
    firebim:hasID "A6.8.5.4.1.1" ;
    firebim:hasMember firebim:Member_6_8_5_4_1_1_1, firebim:Member_6_8_5_4_1_1_2 .

firebim:Member_6_8_5_4_1_1_1 a firebim:Member ;
    firebim:hasOriginalText """Deze   bovengrondse   en   ondergrondse   hydranten   worden   gevoed   door   het   openbaar
waterleidingnet via een leiding met minimale binnendiameter van 80 mm."""@nl .

firebim:Member_6_8_5_4_1_1_2 a firebim:Member ;
    firebim:hasOriginalText """Kan   het   openbaar   net   aan  deze   voorwaarden   niet   voldoen,   dan   wendt   men   andere
bevoorradingsbronnen  aan  met  minimale  capaciteit  van  50 m³,  tenzij  het  gehele  gebouw  is
uitgerust met een automatische blusinstallatie van het type sprinkler die is aangepast aan de
aanwezige risico's."""@nl .

firebim:Section_6_8_5_4_2 a firebim:Section ;
    firebim:hasID "6.8.5.4.2" ;
    firebim:hasArticle firebim:Article_6_8_5_4_2_1 .

firebim:Article_6_8_5_4_2_1 a firebim:Article ;
    firebim:hasID "A6.8.5.4.2.1" ;
    firebim:hasMember firebim:Member_6_8_5_4_2_1_1, firebim:Member_6_8_5_4_2_1_2 .

firebim:Member_6_8_5_4_2_1_1 a firebim:Member ;
    firebim:hasOriginalText """In de nijverheids- en handelszones en op de plaatsen met een grote bevolkingsdichtheid liggen de
wateraansluitingen op een maximum afstand van 100 m van elkaar verwijderd."""@nl .

firebim:Member_6_8_5_4_2_1_2 a firebim:Member ;
    firebim:hasOriginalText """Elders zijn zij
wegens de ligging van de voor brand te beveiligen gebouwen of inrichtingen zo verdeeld dat de
afstand tussen de ingang van elk gebouw of van elke inrichting en de dichtstbijgelegen hydrant
niet meer dan 200 m bedraagt."""@nl .

firebim:Section_6_8_5_4_3 a firebim:Section ;
    firebim:hasID "6.8.5.4.3" ;
    firebim:hasArticle firebim:Article_6_8_5_4_3_1 .

firebim:Article_6_8_5_4_3_1 a firebim:Article ;
    firebim:hasID "A6.8.5.4.3.1" ;
    firebim:hasMember firebim:Member_6_8_5_4_3_1_1 .

firebim:Member_6_8_5_4_3_1_1 a firebim:Member ;
    firebim:hasOriginalText """De  ondergrondse  of  de  bovengrondse  hydranten  worden  aangebracht  op  een  horizontaal
gemeten afstand van ten minste 0,60 m van de kant der straten, wegen of doorgangen waarop
voertuigen kunnen rijden en parkeren."""@nl .